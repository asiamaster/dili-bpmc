      <messageEventDefinition messageRef="deleteRentalOrderMessage"></messageEventDefinition>
    </boundaryEvent>
    <boundaryEvent id="invalidateRentalOrder" name="租赁单作废" attachedToRef="settlementRentalOrder" cancelActivity="true">
